# ESS Detector group FPGA demonstrator database
# $(SYS)    = The System (DG)
# $(DEV) 	= The DEV name (FPGA)
# $(REG0) 	= register offset
# $(COM)	= COM port for stream DEV
# $(PRO) 	= stream DEV protocol filenam



record(mbbo, "$(SYS)-$(DEV):$(SFX)-SP"){
	field(DESC, "Reset FPGA SP")
	field(ZRST, "Enable")
	field(ONST, "Reset")
	field(ZRVL, "0")
	field(ONVL, "18")
	field(DTYP, "Raw Soft Channel")
	field(OUT, "$(SYS)-$(DEV):RST-WR PP")
	info(autosaveFields_pass0, "VAL")
	field(PINI, "1")
	
}

record(longout, "$(SYS)-$(DEV):RST-WR"){
	field(DESC, "Write to Reset Vector")
	field(DTYP, "stream")
	field(OUT, "@ics-dg.proto writeRegister($(REG0),"$(SYS)-$(DEV):$(SFX)-SP") $(COM)")
}




