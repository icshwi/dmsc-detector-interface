# Write strobe register

# ESS Detector group FPGA demonstrator database
# $(SYS)    		= The System (DG)
# $(DEV) 			= The DEV name (FPGA)
# $(REG) 			= name of register
# $(OFF) 			= register offset
# $(COM)			= COM port for stream DEV
# $(PRO) 			= stream DEV protocol filename



## Communication Records ###
record(longin, "$(SYS)-$(DEV):$(REG)-OFF") {
  field(VAL, "$(OFF)")
}

record(bo, "$(SYS)-$(DEV):$(REG)-CMD"){

  field(DESC, "$(REG) write for ${REG}")
  field(DTYP, "stream")
  field(OUT, "@$(PRO) writeRegister($(SYS)-$(DEV),$(REG)-OFF,$(REG)-CMD) $(COM)")
  
}

